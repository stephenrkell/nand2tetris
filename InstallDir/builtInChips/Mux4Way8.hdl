/**
 * 4-way 8-bit multiplexor.  
 * out = a -- if sel=00
 *       b -- if sel=01
 *       c -- if sel=10
 *       d -- if sel=11
 */

CHIP Mux4Way8 {
     
    IN a[8], b[8], c[8], d[8], sel[2];
    OUT out[8];

    BUILTIN Mux4Way8;
}
